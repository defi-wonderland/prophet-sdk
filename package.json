{
  "name": "opoo-sdk",
  "version": "1.0.0",
  "description": "Lightweight and user-friendly wrapper around OpOO contracts",
  "repository": "git@github.com:defi-wonderland/opoo-sdk-private.git",
  "license": "MIT",
  "author": "Wonderland",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn typechain && rm -rf dist && tsc -p tsconfig.json",
    "lint:check": "eslint ./src",
    "lint:fix": "sort-package-json && eslint ./src --fix",
    "test": "cross-env mocha 'test/**/*.spec.ts'",
    "test:coverage": "nyc --reporter=lcov --reporter=text mocha 'test/**/*.spec.ts'",
    "typechain": "typechain --target ethers-v5 --out-dir src/types/typechain './node_modules/opoo-core/abi/*.json'"
  },
  "lint-staged": {
    "*.ts": "eslint ./src/**/*.ts",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "@pinata/sdk": "2.1.0",
    "cids": "1.1.9",
    "ethers": "5.7.0",
    "ipfs-core": "0.18.1",
    "opoo-core": "0.0.0-88e224d8",
    "pinata-upload-cli": "1.1.0"
  },
  "devDependencies": {
    "@ethersproject/abstract-provider": "5.7.0",
    "@ethersproject/abstract-signer": "5.7.0",
    "@typechain/ethers-v5": "11.0.0",
    "@types/chai": "4.3.5",
    "@types/chai-as-promised": "7.1.5",
    "@types/mocha": "10.0.1",
    "@types/node": "20.2.3",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "cross-env": "7.0.3",
    "dotenv": "16.2.0",
    "eslint": "8.40.0",
    "husky": "8.0.3",
    "lint-staged": "10",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "prettier": "2.8.8",
    "sinon": "15.2.0",
    "sort-package-json": "2.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typechain": "8.2.0",
    "typescript": "5.0.4"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
